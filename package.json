        "@angular/forms": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "axios": "^0.24.0",
        "date-fns": "^2.25.0",
        "dotenv": "^16.0.2",
        "express": "^4.17.1",
        "rxjs": "~6.6.0",
        "socket.io": "^4.3.1",
        "socket.io-client": "^4.3.2",
        "systeminformation": "^5.9.9",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
