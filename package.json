{
    "name": "my-magic-screen",
    "version": "1.0.0",
    "module": "commonjs",
    "scripts": {
        "watch": "ng build --watch --configuration development",

        "test:front": "echo [package.json] ng test",
        "test:server": "echo [package.json] test server",

        "dev:front": "ng serve --disable-host-check",
        "dev:server": "nodemon --inspect=9229 server.js",

